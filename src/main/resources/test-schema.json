			"description": "This field is optional",
			"type": "string"
		},
		"cost": {
			"description": "Cost to test",
			"type": "number"
		},
		"active": {
			"description": "Is active",
			"type": "boolean"
		},
		"any_date": {
			"description": "Any date",
			"type": "string",
