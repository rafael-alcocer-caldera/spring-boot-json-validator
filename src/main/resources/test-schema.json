	"title": "Test",
	"description": "Json Test",
	"type": "object",
	"properties": {
	    "id": {
			"description": "Identifier",
			"type": "integer"
		},
		"name": {
			"description": "Name to test",
			"type": "string"
		},
		"description": {
			"description": "Description to test",
			"type": "string"
		},
